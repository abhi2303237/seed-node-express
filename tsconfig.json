{
    "compilerOptions": {
        "skipLibCheck": true,
        "module": "commonjs",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "@services/*": [
                "src/services/*"
            ],
            "@dto/*": [
                "src/DTO/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@module/*": [
                "src/module/*"
            ],
            "@middlewares/*": [
                "src/middlewares/*"
            ],
            "@config/*": [
                "src/config/*"
            ]
        }
    },
    "include": [
        "src/**/*"
    ]
}